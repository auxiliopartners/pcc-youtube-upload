# Environment Configuration
NODE_ENV=development

# Logging
# Set to 'true' to enable debug logging with pretty formatting
DEBUG=false

# 1Password SDK Configuration
# Create a service account at https://developer.1password.com/
# OP_SERVICE_ACCOUNT_TOKEN="your_service_account_token_here"
# Optional: Specify a secret reference to test retrieval (format: op://vault-name/item-name/field-name)
# OP_SECRET_REFERENCE="op://Private/Example Item/password"

# Application Settings
# APP_NAME=my-app
# APP_PORT=3000
# APP_HOST=localhost

# Database (example)
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# API Keys (example)
# API_KEY=your_api_key_here
# API_SECRET=your_api_secret_here
